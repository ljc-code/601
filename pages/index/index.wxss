/**index.wxss**/
page{
  background-color: #f7f7f7;
}
.topBg{
  width: 100%;
  height: 320rpx;
  /* position: absolute;
  left: 0;
  top: -100rpx; */
}
.allCont{
  width: 100%;
  box-sizing: border-box;
  position: absolute;
  top: 0;
  left: 0;
  padding: 0 24rpx 78rpx 24rpx;
}
.topIcon{
  display: flex;
  justify-content: flex-end;
  align-items: center;
  margin-top: 30rpx;
  margin-bottom: 40rpx;
}
.message{
  width: 40rpx;
  height: 42rpx;
  position: relative;
  margin: 0 43rpx;
}
.redNum{
  position: absolute;
  top: -10rpx;
  right: 0rpx;
  width: 20rpx;
  height: 20rpx;
  line-height: 20rpx;
  text-align: center;
  background: #FF7E7E;
  border-radius: 50%;
  font-size: 18rpx;
  font-family: DINPro;
  font-weight: 500;
  color: #FFFFFF;
}
.indexIcon1,.indexIcon3{
  width: 40rpx;
  height: 40rpx;
}
.indexIcon2{
  width: 40rpx;
  height: 42rpx;
  position: absolute;
  top: 0rpx;
  right: 0rpx;
}
.peopleName{
  font-size: 34rpx;
  font-family: PingFang;
  /* font-weight: bold; */
  color: #FFFFFF;
  /* margin-bottom: 98rpx; */
}
.noRegister{
  display: flex;
  align-items: center;
  flex-direction: column;
  text-align: left;
  width: 622rpx!important;
  background-color: #FFFFFF;
  font-size: 30rpx;
  margin: 0;
  padding: 0;
}
.pleaseRegister{
  font-weight: bold;
  margin-bottom: 10rpx;
}
.clickword{
  color: #666666;
}
.quit{
  width: 120rpx;
  height: 50rpx;
  font-size: 30rpx;
  line-height: 50rpx;
  text-align: center;
  background-color: #999999;
  color: #FFFFFF;
  border-radius: 20rpx;
}

.company{
  width: 702rpx;
  height: 180rpx;
  padding: 0 40rpx;
  padding-top: 40rpx;
  box-sizing: border-box;
  background-color: #FFFFFF;
  box-shadow: 0rpx 10rpx 20rpx 0rpx rgba(80, 94, 146, 0.2);
  border-radius: 50rpx;
}
.companyName{
  font-size: 30rpx;
  font-family: PingFang;
  font-weight: bold;
  color: #333333;
  display: flex;
  justify-content: space-between;
  align-items: center;
  position: relative;
}
.arrows{
  width: 15rpx;
  height: 9rpx;
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 0;
}
.department{
  margin-top: 10rpx;
}
.department text{
  font-size: 26rpx;
  font-family: PingFang;
  font-weight: 400;
  color: #333333;
}

.property{
  width: 702rpx;
  /* height: 223rpx; */
  padding: 32rpx;
  box-sizing: border-box;
  background-color: #FFFFFF;
  border-radius: 30rpx;
  margin-top: 32rpx;
}
.myProperty{
  font-size: 30rpx;
  font-family: PingFang;
  font-weight: 400;
  color: #333333;
  margin-bottom: 20rpx;
  margin-left: 26rpx
}
.classify{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 50rpx;
}
.classifyChild{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
}
.number{
  font-size: 40rpx;
  font-family: DINPro;
  font-weight: 500;
  color: #333333;
  height: 40rpx;
  line-height: 40rpx;
}
.propertyStaty{
  font-size: 26rpx;
  font-family: PingFang;
  font-weight: 400;
  color: #666666;
  margin-top: 20rpx;
}
.manage{
  width: 702rpx;
  /* height: 397rpx; */
  background: #FFFFFF;
  border-radius: 30rpx;
  padding: 32rpx 0 32rpx 35rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
}
.myManage{
  font-size: 30rpx;
  font-family: PingFang;
  font-weight: 400;
  margin-left: 26rpx;
  color: #333333;
}
.manageAll{
  display: flex;
  justify-content: space-between;
  align-items: center;
  flex-wrap: wrap;
}
.manageAll::after{
  content: "";
  width: 170rpx;
}
.manageChild{
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  width: 170rpx;
  margin-top: 40rpx;
}
.manageIcon{
  width: 64rpx;
  height: 64rpx;
}


.consume{
  width: 702rpx;
  /* height: 244rpx; */
  background: #FFFFFF;
  border-radius: 30rpx;
  padding: 32rpx 0 32rpx 35rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
}
.pageTop{
  /* height: 225rpx; */
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 100rpx;
}

.picker{
  width: 622rpx;
}

.quitBtn{
  font-size: 30rpx;
  color: #FFFFFF;
  margin-right: 30rpx;
}